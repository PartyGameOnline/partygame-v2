{
  "name": "partygame-v2",
  "repository": {
    "type": "git",
    "url": "https://github.com/PartyGameOnline/partygame-v2.git"
  },
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20 <21"
  },
  "packageManager": "npm@10.9.3",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "check:lock": "node scripts/check-lock.js --verbose",
    "check:lock:ci": "node scripts/check-lock.js --ci",
    "spec": "tsx src/game/spec.ts",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "prepare": "husky",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "echo \"no tests yet\" && exit 0",
    "check": "npm run lint && npm run typecheck",
    "licenses:summary": "license-checker-rseidelsohn --production --summary",
    "licenses:csv": "license-checker-rseidelsohn --production --csv --out licenses.csv",
    "smoke:counter": "tsx scripts/smoke-counter.ts"
  },
  "lockcheck": {
    "allow": [
      "ansi-regex",
      "debug"
    ],
    "deny": []
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --max-warnings=0",
    "*.{js,ts,tsx,css,md,json}": "prettier --write"
  },
  "dependencies": {
    "next": "15.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/css": "^0.11.1",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "9.37.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.3.0",
    "@next/eslint-plugin-next": "15.5.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "24.8.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "autoprefixer": "^10.4.21",
    "critters": "0.0.23",
    "eslint": "9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "license-checker-rseidelsohn": "4.4.2",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.14",
    "ts-node": "10.9.2",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0"
  },
  "overrides": {
    "@eslint/core": "0.16.0",
    "@eslint/plugin-kit": "0.4.0",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "@typescript-eslint/project-service": "8.45.0",
    "@typescript-eslint/scope-manager": "8.45.0",
    "@typescript-eslint/tsconfig-utils": "8.45.0",
    "@typescript-eslint/types": "8.45.0",
    "@typescript-eslint/typescript-estree": "8.45.0",
    "@typescript-eslint/utils": "8.45.0",
    "@typescript-eslint/visitor-keys": "8.45.0"
  }
}
